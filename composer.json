{
    "name": "devgroup/yii2-intent-analytics",
    "description": "Allows building yii2 apps for multiple languages using regional URL's and domains",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","analytics", "metrika", "google analytics", "intent", "statistics"],
    "homepage": "http://www.devgroup.ru/",
    "support": {
        "issues": "https://github.com/devgroup-ru/yii2-intent-analytics/issues?state=open",
        "source": "https://github.com/devgroup-ru/yii2-intent-analytics"
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Alexander Kozhevnikov",
            "email": "b37hr3z3n@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "^2.0.6",
        "jisoft/yii2-sypexgeo": "dev-master",
        "devgroup/yii2-tag-dependency-helper": "^1.2.0",
        "devgroup/yii2-multilingual-sypexgeodaemon": "dev-master",
        "devgroup/yii2-multilingual": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phpunit/dbunit": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "DevGroup\\Analytics\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DevGroup\\Analytics\\Tests\\": "tests/"
        }
    },

    "extra":{
      "migrationPath": [
        "src/migrations"
      ]
    }
}
