{
    "name": "devgroup/yii2-intent-analytics",
    "description": "Visitors intent analytics and integration with Google Analytics and Yandex.Metrika for Yii framework 2",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","analytics", "metrika", "google analytics", "intent", "statistics"],
    "homepage": "http://www.devgroup.ru/",
    "support": {
        "issues": "https://github.com/devgroup-ru/yii2-intent-analytics/issues?state=open",
        "source": "https://github.com/devgroup-ru/yii2-intent-analytics"
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Alexander Kozhevnikov",
            "email": "b37hr3z3n@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "^2.0.6",
        "devgroup/yii2-tag-dependency-helper": "~1.4.0",
        "nixsolutions/yandex-php-library" : "dev-master",
        "google/apiclient" : "^2.0"
    },
    "require-dev": {
        "devgroup/dotplant-dev": "*"
    },
    "autoload": {
        "psr-4": {
            "DevGroup\\Analytics\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DevGroup\\Analytics\\Tests\\": "tests/"
        }
    },
    "extra": {
        "yii2-extension": {
            "name": "Analytics",
            "name_ru": "Аналитика",
            "iconUrl": "https://st-1.dotplant.ru/images/ext-sample.png",
            "description": "Visitors intent analytics and integration with Google Analytics and Yandex.Metrika for Yii framework 2",
            "description_ru": "Аналитика посещаемости с интеграцией с Google Analytics и Yandex.Metrika для Yii framework 2"
        },
        "migrationPath": ["src/migrations/"],
        "translationCategory": "extension.analytics"
    }
}
